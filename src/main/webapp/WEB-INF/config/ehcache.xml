<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
	<diskStore path="java.io.tmpdir/EhCache" />
	
    <defaultCache eternal="true" maxElementsInMemory="100" overflowToDisk="false" />
    <cache name="routesCache" maxElementsInMemory="3" eternal="true" overflowToDisk="false" />
    <cache name="airlinesCache" maxElementsInMemory="3" eternal="true" overflowToDisk="false" />
    <cache name="countriesCache" maxElementsInMemory="1" eternal="true" overflowToDisk="false" />
    <cache name="allAirportsCache" maxElementsInMemory="1" eternal="true" overflowToDisk="false" />
    <cache name="airportsCache" maxElementsInMemory="10" eternal="true" overflowToDisk="false" />
    <cache name="countByCountryCache" maxElementsInMemory="10" eternal="true" overflowToDisk="false" />
    <cache name="searchAirportsCache" maxElementsInMemory="50" eternal="true" overflowToDisk="false" />
</ehcache>